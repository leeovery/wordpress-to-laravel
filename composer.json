{
  "name": "leeovery/wordpress-to-laravel",
  "type": "library",
  "description": "",
  "keywords": [
    "LeeOvery",
    "WordpressToLaravel",
    "leeovery",
    "wordpress-to-laravel"
  ],
  "homepage": "https://github.com/leeovery/wordpress-to-laravel",
  "license": "MIT",
  "authors": [
    {
      "name": "Lee Overy",
      "email": "me@leeovery.com",
      "homepage": "https://github.com/leeovery",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "~7.0",
    "cartalyst/tags": "4.0.*",
    "guzzlehttp/guzzle": "^6.2",
    "illuminate/database": "5.3.*|5.4.*|5.5.*",
    "illuminate/support": "5.3.*|5.4.*|5.5.*",
    "illuminate/console": "5.3.*|5.4.*|5.5.*",
    "illuminate/contracts": "5.3.*|5.4.*|5.5.*",
    "illuminate/events": "5.3.*|5.4.*|5.5.*",
    "league/fractal": "^0.14.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.0||~5.0"
  },
  "autoload": {
    "psr-4": {
      "LeeOvery\\WordpressToLaravel\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "LeeOvery\\WordpressToLaravel\\": "tests"
    }
  },
  "scripts": {
    "test": "phpunit",
    "check-style": "phpcs -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src tests",
    "fix-style": "phpcbf -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src tests"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  },
  "config": {
    "sort-packages": true
  }
}
